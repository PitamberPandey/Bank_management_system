<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean name ="emp1" class="Spring.collection.Emp">
        <property name="name" value="Pitamber Pandey"/>
        <property name="phones">
            <list>
                <value>983996002002</value>
                <value>9857544678778</value>
                <value>9875754556</value>
            </list>
        </property>
        <property name="address">
            <set>
                <value>Delhi</value>
                <value>Nepal</value>
                <value>India</value>
            </set>
        </property>
        <property name="courses">
            <map>
                <entry key="java" value="2month"/>
                <entry key="python" value="3month"/>
                <entry key="javascript" value="1month"/>
            </map>
        </property>
    </bean>

</beans>
